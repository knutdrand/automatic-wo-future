        {
            "name": "3. Model Training",
            "item": [
                {
                    "name": "Train Model",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "if (pm.response.code === 200 || pm.response.code === 202) {",
                                    "    const response = pm.response.json();",
                                    "    pm.collectionVariables.set('train_job_id', response.job_id);",
                                    "    pm.collectionVariables.set('model_artifact_id', response.artifact_id);",
                                    "}"
                                ],
                                "type": "text/javascript"
                            }
                        }
                    ],
                    "request": {
                        "method": "POST",
                        "header": [{"key": "Content-Type", "value": "application/json"}],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"config_id\": \"{{config_id}}\",\n  \"data\": {\n    \"columns\": [\"feature_1\", \"feature_2\", \"target\"],\n    \"data\": [\n      [1.0, 2.0, 3.0],\n      [2.0, 3.0, 5.0],\n      [3.0, 4.0, 7.0],\n      [4.0, 5.0, 9.0]\n    ]\n  }\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/api/v1/ml/$train",
                            "host": ["{{baseUrl}}"],
                            "path": ["api", "v1", "ml", "$train"]
                        }
                    }
                },
                {
                    "name": "Poll Training Job",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{baseUrl}}/api/v1/jobs/{{train_job_id}}",
                            "host": ["{{baseUrl}}"],
